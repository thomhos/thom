{
  "name": "thom",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "npm-run-all --parallel start:*",
    "start:serve": "browser-sync start --server 'build' --files 'build'",
    "start:watch": "onchange 'src/**.*' -- npm run build",
    "prebuild": "rimraf build && mkdir -p build",
    "build": "copyfiles -f src/index.html build && browserify src/index.js -t yo-yoify -t [ sheetify/transform -u sheetify-cssnext ] -p [ css-extract -o build/bundle.css ] src/index.js -g [ babelify --presets [ es2015 ] ] -o build/bundle.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "budo": "^9.1.0",
    "copyfiles": "^1.0.0",
    "css-extract": "^1.1.2",
    "envify": "^3.4.1",
    "npm-run-all": "^3.1.0",
    "onchange": "^3.0.2",
    "sheetify-cssnext": "^1.0.7",
    "uglify-js": "^2.7.3",
    "uglifyify": "^3.0.3",
    "unassertify": "^2.0.3",
    "yo-yoify": "^3.4.1"
  },
  "dependencies": {
    "choo": "^3.3.0",
    "sheetify": "^5.1.0"
  }
}
