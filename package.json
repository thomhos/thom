{
  "name": "thom",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "predev": "npm-run-all --parallel build:*",
    "dev": "npm run serve",
    "serve": "nodemon -w client -w server -e js,json,scss --exec \"npm-run-all --silent --parallel build:* && babel-node build\"",
    "prebuild": "rimraf build && mkdir -p build && npm-run-all copy:*",
    "build:client": "browserify client/index.js -t yo-yoify -t [ sheetify/transform -u sheetify-sass -u sheetify-cssnext ] -p [ css-extract -o build/bundle.css ] client/index.js -g [ babelify --presets [ es2015 ] ] -o build/bundle.js",
    "build:server": "babel server -s -D -d build --presets es2015,stage-0",
    "copy:html": "copyfiles -f client/index.html build",
    "copy:fonts": "copyfiles -f client/assets/fonts/* build/fonts",
    "copy:translations": "copyfiles -f client/assets/translations/* build/translations",
    "prestart": "npm-run-all --parallel build:*",
    "start": "node build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "budo": "^9.1.0",
    "copyfiles": "^1.0.0",
    "css-extract": "^1.1.2",
    "envify": "^3.4.1",
    "nodemon": "^1.10.2",
    "npm-run-all": "^3.1.0",
    "onchange": "^3.0.2",
    "rfr": "^1.2.3",
    "rimraf": "^2.5.4",
    "sheetify-cssnext": "^1.0.7",
    "sheetify-sass": "^1.0.1",
    "sheetify-variables": "^1.0.4",
    "uglify-js": "^2.7.3",
    "uglifyify": "^3.0.3",
    "unassertify": "^2.0.3",
    "yo-yoify": "^3.4.1"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "bel": "^4.4.3",
    "body-parser": "^1.15.2",
    "choo": "^3.3.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "i18next": "^3.4.2",
    "keystone": "^4.0.0-beta.2",
    "multer": "0.1.8",
    "sheetify": "^5.1.0"
  }
}
